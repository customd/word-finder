{
  "name": "custom-d/word-finder",
  "description": "A word finder",
  "license": "MIT",
  "keywords": [
    "word",
    "finder"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Craig Smith",
      "email": "craig.smith@customd.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "CustomD\\WordFinder\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CustomD\\WordFinder\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CustomD\\WordFinder\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
